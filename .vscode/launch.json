{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Client Jest",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "args": ["${file}", "--runInBand", "--no-cache", "--config", "jest.config.client.js"],
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Server Jest",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "args": ["${file}", "--runInBand", "--no-cache", "--config", "jest.config.js"],
      "cwd": "${workspaceRoot}/packages/server/management-api",
      "console": "integratedTerminal"
    }
  ]
}
